                            </TouchableHighlight>
                           
                        </View>
                            <Text style={{fontSize: deviceHeight/20, }}>
                                Guesses
                            </Text>
                            <View style={styles.settingsLine}>
                            </View>
                        <View style={styles.optionsContainer}>
                            <TextInput
                                value={this.state.guesses}
                                onChangeText={this._handleTextChange}
                                maxLength={2}
                                keyBoardType={'numeric'}
                                
                                style={{ width: deviceWidth/4, height: deviceWidth/4, padding: 8, borderWidth: 1, fontSize: deviceHeight/18, textAlign: 'center'}}
                            />
                            
                        
                        </View>
                            <Text style={{fontSize: deviceHeight/20, }}>
                                Words at a Time
                            </Text>
                            <View style={styles.settingsLine}>
                            </View>
                        <View style={styles.optionsContainer}>
                            <TouchableHighlight style={{height: deviceWidth/5, width: deviceWidth/5, borderWidth: 4, marginRight: 14, borderRadius: 90, alignItems: 'center', justifyContent: 'center', backgroundColor: this.state.selectedG1}}
                                onPress={() => this.numGameSelect(1)}
                            >
                                <Text style={{fontSize: deviceHeight/22}}>
                                    1
                                </Text>
                                
                            </TouchableHighlight>
                            <TouchableHighlight style={{height: deviceWidth/5, width: deviceWidth/5, borderWidth: 4, marginRight: 14, borderRadius: 90, alignItems: 'center', justifyContent: 'center', backgroundColor: this.state.selectedG2}}
                                onPress={() => this.numGameSelect(2)}
                            >
                                <Text style={{fontSize: deviceHeight/22}}>
                                    2
                                </Text>
                                
                            </TouchableHighlight>
                            <TouchableHighlight style={{height: deviceWidth/5, width: deviceWidth/5, borderWidth: 4, marginRight: 14, borderRadius: 90, alignItems: 'center', justifyContent: 'center', backgroundColor: this.state.selectedG3}}
                                onPress={() => this.numGameSelect(3)}
                            >
                                <Text style={{fontSize: deviceHeight/22}}>
                                    3
                                </Text>
                                
                            </TouchableHighlight>
                            <TouchableHighlight style={{height: deviceWidth/5, width: deviceWidth/5, borderWidth: 4, marginRight: 14, borderRadius: 90, alignItems: 'center', justifyContent: 'center', backgroundColor: this.state.selectedG4}}
                                onPress={() => this.numGameSelect(4)}
                            >
                                <Text style={{fontSize: deviceHeight/22}}>
                                    4
                                </Text>
                                
                            </TouchableHighlight>
                            
                        </View>
                        
                    </View>
                    
                </View>
                <View style={{display: this.state.overlay}}>
                    <View style={styles.overlayContainer}>
                        <View style={styles.overlay}>
                            <Text style={{fontSize: deviceHeight/18, color: this.state.resultColor, fontWeight: 'bold'}}>
                                {this.state.randomWord.toUpperCase()}
                            </Text>
                            <Text style={{fontSize: deviceHeight/34, color: this.state.resultColor, fontWeight: 'bold', marginTop: 40, marginBottom: 40}}>
                                {this.state.endMessege}
                            </Text>
                            <View style={{flexDirection: 'row', justifyContent: 'center', alignItems: 'center'}}>
                                <TouchableHighlight style={{height: deviceHeight/1.5/6, width: deviceWidth/1.2/3, backgroundColor: '#c9c9c9', borderWidth: 1, justifyContent: 'center', alignItems: 'center', borderRadius: 45, marginRight: 20}}
                                    onPress={this.handlePlayAgainPress}
                                >
                                    <Text style={styles.paragraph}>
                                        Play Again
                                    </Text>
                                
                                </TouchableHighlight>
                                <TouchableHighlight style={{height: deviceHeight/1.5/6, width: deviceWidth/1.2/3, backgroundColor: '#c9c9c9', borderWidth: 1, justifyContent: 'center', alignItems: 'center', borderRadius: 45}}
                                    onPress={this.handleMenuPress}
                                >
                                    <Text style={styles.paragraph}>
                                        Menu
                                    </Text>
                                
                                </TouchableHighlight>
                            
                            </View>
                            
                        
                        </View>
                    
                    </View>
                </View>
                
            </View>
        );
    }
}

