 'tayra', 'teach', 'teaks', 'teals', 'teams', 'tears', 'teary', 'tease', 'teats', 'tebet', 'techy', 'teddy', 'teems', 'teens', 'teeny', 'teeth', 'teffs', 'teiid', 'telex', 'tells', 'telly', 'tempo', 'temps', 'tempt', 'tench', 'tends', 'tenet', 'tenia', 'tenno', 'tenon', 'tenor', 'tense', 'tenth', 'tents', 'tepal', 'tepee', 'tepid', 'teras', 'terce', 'teres', 'terms', 'terns', 'terry', 'terse', 'tesla', 'testa', 'tests', 'testy', 'teths', 'teton', 'tetra', 'texan', 'texas', 'texts', 'thane', 'thank', 'thats', 'thaws', 'theca', 'theft', 'theme', 'thens', 'there', 'therm', 'theta', 'thick', 'thief', 'thigh', 'thill', 'thing', 'think', 'thins', 'third', 'thole', 'thong', 'thorn', 'three', 'thrip', 'throb', 'throe', 'throw', 'thrum', 'thuds', 'thugs', 'thuja', 'thule', 'thumb', 'thump', 'thyme', 'tiara', 'tiber', 'tibet', 'tibia', 'tical', 'ticks', 'tidal', 'tides', 'tiers', 'tiffs', 'tifli', 'tiger', 'tight', 'tigon', 'tikes', 'tilde', 'tiled', 'tiler', 'tiles', 'tilia', 'tills', 'tilth', 'tilts', 'timed', 'timer', 'times', 'timid', 'timor', 'tinct', 'tinea', 'tined', 'tines', 'tinge', 'tings', 'tinny', 'tints', 'tipis', 'tippy', 'tipsy', 'tired', 'tires', 'titan', 'titer', 'tithe', 'titis', 'title', 'titre', 'titty', 'titus', 'tizzy', 'toads', 'toady', 'toast', 'today', 'toddy', 'todea', 'todus', 'toffs', 'toffy', 'tokay', 'token', 'tokes', 'tokyo', 'toles', 'tolls', 'tombs', 'tomes', 'tonal', 'toned', 'toner', 'tones', 'tonga', 'tongs', 'tonic', 'tonne', 'tonus', 'tools', 'toona', 'toons', 'tooth', 'topaz', 'topee', 'toper', 'topes', 'topic', 'topis', 'topos', 'toque', 'torah', 'torch', 'tores', 'torsk', 'torso', 'torte', 'torts', 'torus', 'total', 'totem', 'toter', 'totes', 'touch', 'tough', 'tours', 'touts', 'towel', 'tower', 'towns', 'towny', 'toxic', 'toxin', 'toyon', 'trace', 'track', 'tract', 'tracy', 'trade', 'trail', 'train', 'trait', 'tramp', 'trams', 'trapa', 'trash', 'trave', 'trawl', 'trays', 'tread', 'treat', 'treed', 'trees', 'treks', 'trema', 'trend', 'trent', 'tress', 'trews', 'treys', 'triad', 'trial', 'tribe', 'trice', 'trick', 'tried', 'trier', 'tries', 'triga', 'trigs', 'trike', 'trill', 'trims', 'trine', 'tripe', 'trips', 'trite', 'troat', 'troll', 'troop', 'trope', 'troth', 'trots', 'trout', 'trove', 'troys', 'truce', 'truck', 'trues', 'truly', 'trump', 'trunk', 'truss', 'trust', 'truth', 'tryst', 'tsars', 'tsine', 'tsuga', 'tubal', 'tubas', 'tubby', 'tubed', 'tuber', 'tucks', 'tudor', 'tufas', 'tuffs', 'tufts', 'tulip', 'tulle', 'tulsa', 'tumid', 'tummy', 'tumor', 'tunas', 'tuner', 'tunes', 'tunga', 'tungs', 'tunic', 'tunis', 'tunny', 'tupek', 'tupik', 'turds', 'turfs', 'turki', 'turks', 'turns', 'turps', 'tushs', 'tusks', 'tutee', 'tutor', 'tuxes', 'tuxub', 'twain', 'twang', 'twats', 'tweak', 'tweed', 'tweet', 'twerp', 'twice', 'twigs', 'twill', 'twine', 'twins', 'twirl', 'twirp', 'twist', 'twits', 'tyche', 'tying', 'tykes', 'tyler', 'tynes', 'types', 'typha', 'typic', 'typos', 'tyres', 'tyros', 'tzars', 'udder', 'uglis', 'ugric', 'uigur', 'ukase', 'ulama', 'ulcer', 'ulema', 'ulmus', 'ulnar', 'ulnas', 'ultra', 'ulvas', 'umbel', 'umber', 'umbos', 'umbra', 'unais', 'unarm', 'unary', 'unaus', 'unbar', 'unbox', 'uncle', 'uncos', 'uncus', 'uncut', 'under', 'undue', 'unfed', 'unfit', 'uniat', 'unify', 'union', 'unite', 'units', 'unity', 'unlax', 'unlit', 'unman', 'unpin', 'unsay', 'unsex', 'untie', 'until', 'unwed', 'unzip', 'upend', 'upper', 'upset', 'upupa', 'urate', 'urban', 'ureas', 'urges', 'uriah', 'urial', 'urine', 'ursus', 'usage', 'users', 'ushas', 'usher', 'using', 'usnea', 'usual', 'usurp', 'usury', 'uteri', 'utile', 'utter', 'uveal', 'uveas', 'uvula', 'uzbak', 'uzbeg', 'uzbek', 'vagal', 'vague', 'vagus', 'vajra', 'vales', 'valet', 'valid', 'valmy', 'valor', 'valse', 'value', 'valve', 'vamps', 'vanda', 'vaned', 'vanes', 'vanir', 'vapid', 'vapor', 'varan', 'varix', 'varna', 'varus', 'vases', 'vasts', 'vatic', 'vault', 'vaunt', 'veals', 'vedic', 'veers', 'veery', 'vegan', 'veils', 'veins', 'velar', 'velds', 'veldt', 'velum', 'venal', 'vends', 'venom', 'vents', 'venue', 'venus', 'vepse', 'verbs', 'verdi', 'verge', 'verpa', 'verse', 'verso', 'verst', 'vertu', 'verve', 'vespa', 'vesta', 'vests', 'vetch', 'vexed', 'vexer', 'vexes', 'vials', 'viand', 'vibes', 'vicar', 'vices', 'vichy', 'vicia', 'video', 'vidua', 'views', 'vigil', 'vigor', 'villa', 'vinca', 'vines', 'vinos', 'vinyl', 'viola', 'viols', 'viper', 'viral', 'vireo', 'virga', 'virgo', 'virtu', 'virus', 'visas', 'vises', 'visit', 'visod', 'visor', 'vista', 'vital', 'vitis', 'vivas', 'vivid', 'vixen', 'vizor', 'vocal', 'vodka', 'vogue', 'vogul', 'voice', 'voids', 'voile', 'volar', 'voles', 'volga', 'volta', 'volts', 'volva', 'vomer', 'vomit', 'voter', 'votes', 'vouch', 'vouge', 'vowel', 'vower', 'vroom', 'vulva', 'wacky', 'wader', 'wades', 'wadis', 'wafer', 'wafts', 'wager', 'wages', 'wagon', 'wahoo', 'waifs', 'wails', 'wains', 'waist', 'waits', 'waive', 'waken', 'waker', 'wakes', 'wales', 'walks', 'walls', 'wally', 'waltz', 'wands', 'wanes', 'wanly', 'wants', 'wapmo', 'wards', 'warms', 'warns', 'warps', 'warts', 'warty', 'washy', 'wasps', 'waste', 'watch', 'water', 'watts', 'waugh', 'wauls', 'waver', 'wawls', 'waxed', 'waxen', 'waxes', 'wayne', 'weald', 'weals', 'weans', 'wears', 'weary', 'weave', 'webby', 'weber', 'wedel', 'wedge', 'weeds', 'weedy', 'weeks', 'weeny', 'weeps', 'weepy', 'wefts', 'weigh', 'weird', 'weirs', 'wekas', 'welch', 'welds', 'wells', 'welsh', 'welts', 'wench', 'wends', 'wests', 'whack', 'whale', 'whams', 'whang', 'whaps', 'wharf', 'whats', 'wheal', 'wheat', 'wheel', 'whelk', 'whelm', 'whelp', 'whets', 'wheys', 'whiff', 'whigs', 'while', 'whims', 'whine', 'whins', 'whiny', 'whirl', 'whirr', 'whirs', 'whish', 'whisk', 'whist', 'white', 'whits', 'whizz', 'whole', 'whomp', 'whoop', 'whops', 'whore', 'whorl', 'whose', 'wicca', 'wicks', 'widen', 'wides', 'widow', 'width', 'wield', 'wifes', 'wight', 'wilds', 'wiles', 'wince', 'winch', 'winds', 'windy', 'wines', 'winey', 'wings', 'winks', 'wiper', 'wipes', 'wired', 'wirer', 'wires', 'wises', 'wisps', 'wispy', 'witch', 'withe', 'withy', 'witty', 'wizen', 'woads', 'woden', 'wolfs', 'wolof', 'woman', 'wombs', 'wonky', 'wonts', 'woods', 'woody', 'wooer', 'woofs', 'woolf', 'wools', 'wooly', 'woosh', 'woozy', 'words', 'wordy', 'works', 'world', 'wormy', 'worry', 'worse', 'worst', 'worth', 'worts', 'wospy', 'wound', 'woven', 'wrack', 'wraps', 'wrath', 'wrawl', 'wreak', 'wreck', 'wrest', 'wrick', 'wries', 'wring', 'wrist', 'write', 'writs', 'wrong', 'wroth', 'wryly', 'xenon', 'xeric', 'xerox', 'xviii', 'xxiii', 'xylem', 'xylol', 'xyris', 'yacca', 'yacht', 'yacks', 'yagis', 'yahoo', 'yakut', 'yanan', 'yangs', 'yanks', 'yards', 'yarns', 'yaups', 'yawls', 'yawns', 'yawps', 'yazoo', 'yearn', 'years', 'yeast', 'yells', 'yelps', 'yemen', 'yenta', 'yeses', 'yetis', 'yield', 'ylems', 'yobbo', 'yodel', 'yodhs', 'yogic', 'yogis', 'yokel', 'yokes', 'yolks', 'yores', 'young', 'youth', 'yowls', 'yquem', 'yuans', 'yucca', 'yucky', 'yukon', 'yules', 'yuman', 'yummy', 'yurts', 'yussa', 'zaire', 'zakat', 'zaman', 'zamia', 'zapus', 'zarfs', 'zayin', 'zeals', 'zebra', 'zensu', 'zeros', 'zests', 'zesty', 'zetas', 'zilch', 'zills', 'zincs', 'zings', 'zippy', 'zitis', 'zloty', 'zombi', 'zonal', 'zones', 'zooid', 'zooms', 'zoril', 'zunis'];
const guessArray5 = [{row: ['','','','',''], color: ['white','white','white','white','white']}];
const guessArray4 = [{row: ['','','',''], color: ['white','white','white','white']}];
const guessArray3 = [{row: ['','',''], color: ['white','white','white']}];
export default class App extends Component {

    state = {
        homeMenuPage: 'block',
        wordleSettingsPage: 'none',
        originalWordlePage: 'none',
        originalWordlePage4: 'none',
        originalWordlePage3: 'none',
        wordleRules: 'none',
        overlay: 'none',
        randomWord: '',
        wordLength: 5,
        guesses: 6,
        games: 1,
        currentRow: 0,
        currentCol: 0,
        
        selectedL3: 'white',
        selectedL4: 'white',
        selectedL5: '#deebcc',
        
        selectedG1: '#deebcc',
        selectedG2: 'white',
        selectedG3: 'white',
        selectedG4: 'white',
        
        resultColor: '',
        endMessege: '',
        
        showGuessArray5: 
        [
            {row: ['','','','',''], color: ['white','white','white','white','white']}
        ],
        showGuessArray4: 
        [
            {row: ['','','',''], color: ['white','white','white','white']}
        ],
        showGuessArray3: 
        [
            {row: ['','',''], color: ['white','white','white']}
        ],
        
        letters1Row: [
            {letter: 'Q'},
            {letter: 'W'},
            {letter: 'E'},
            {letter: 'R'},
            {letter: 'T'},
            {letter: 'Y'},
            {letter: 'U'},
            {letter: 'I'},
            {letter: 'O'},
            {letter: 'P'},
        ],
        letters2Row: [
            {letter: 'A'},
            {letter: 'S'},
            {letter: 'D'},
            {letter: 'F'},
            {letter: 'G'},
            {letter: 'H'},
            {letter: 'J'},
            {letter: 'K'},
            {letter: 'L'},
        ],
        letters3Row: [
            {letter: 'Z'},
            {letter: 'X'},
            {letter: 'C'},
            {letter: 'V'},
            {letter: 'B'},
            {letter: 'N'},
            {letter: 'M'},
        ]
    }
    handleWordlePagePress = () => this.setState(state => ({
        homeMenuPage: 'none',
        originalWordlePage: 'block',
    }));
    handleWordlePagePress4 = () => this.setState(state => ({
        homeMenuPage: 'none',
        originalWordlePage4: 'block',
    }));
    handleWordlePagePress3 = () => this.setState(state => ({
        homeMenuPage: 'none',
        originalWordlePage3: 'block',
    }));
    handleWordleSettingsPagePress = () => this.setState(state => ({
        homeMenuPage: 'none',
        wordleSettingsPage: 'block',
    }));
    handleWordleRulesPagePress = () => this.setState(state => ({
        homeMenuPage: 'none',
        wordleRules: 'block',
    }));
    handleWordle5Back = () => {
        guessArray5 = [{row: ['','','','',''], color: ['white','white','white','white','white']}];
        this.setState(state => ({
        homeMenuPage: 'block',
        originalWordlePage: 'none',
        currentRow: 0,
        currentCol: 0,
        }));
    }
    handleWordle4Back = () => {
        guessArray4 = [{row: ['','','',''], color: ['white','white','white','white']}];
        this.setState(state => ({
        homeMenuPage: 'block',
        originalWordlePage4: 'none',
        currentRow: 0,
        currentCol: 0,
        }));
    }
    handleWordle3Back = () => {
        guessArray3 = [{row: ['','',''], color: ['white','white','white']}];
        this.setState(state => ({
        homeMenuPage: 'block',
        originalWordlePage3: 'none',
        currentRow: 0,
        currentCol: 0,
        }));
    }
    handleSettingsBack = () => this.setState(state => ({
        homeMenuPage: 'block',
        wordleSettingsPage: 'none',
    }));
    handleRulesBack = () => this.setState(state => ({
        homeMenuPage: 'block',
        wordleRules: 'none',
    }));
    handlePlayAgainPress = () => {
        this.setState(state => ({
            overlay: 'none',
            currentRow: 0,
            currentCol: 0,
        }))
        guessArray5 = [{row: ['','','','',''], color: ['white','white','white','white','white']}];
        guessArray4 = [{row: ['','','',''], color: ['white','white','white','white']}];
        guessArray3 = [{row: ['','',''], color: ['white','white','white']}];
        this.gameStart();
    }
    handleMenuPress = () => {
        this.setState(state => ({
            overlay: 'none',
            homeMenuPage: 'block',
            currentRow: 0,
            currentCol: 0,
        }))
        guessArray5 = [{row: ['','','','',''], color: ['white','white','white','white','white']}];
        guessArray4 = [{row: ['','','',''], color: ['white','white','white','white']}];
        guessArray3 = [{row: ['','',''], color: ['white','white','white']}];
    }

    _handleTextChange = guess => {
        this.setState({ guess });
    };
    _handleTextChange = guesses => {
        this.setState({ guesses });
    };
    outcome = (arr) => {
        let newStr = this.state.randomWord.toUpperCase();
        let str = newStr.split("");
            if(str == arr[this.state.currentRow].row)
                {
                    this.setState(state => ({
                        overlay: 'block',
                        originalWordlePage: 'none',
                        originalWordlePage3: 'none',
                        originalWordlePage4: 'none',
                        resultColor: '#60a370',
                        endMessege: 'Congrats you guessed Correctly',
                    }))
                }
            else if(arr.length-1 == this.state.currentRow)
                {
                    this.setState(state => ({
                        overlay: 'block', 
                        originalWordlePage: 'none',
                        originalWordlePage3: 'none',
                        originalWordlePage4: 'none',
                        resultColor: '#ba0600',
                        endMessege: 'Wrong, how unfortunate'
                    }))
                }
    };
    rGWord = () => {
      if(this.state.wordLength == 5)
      {
          this.setState(state => ({
                randomWord: words[Math.floor(Math.random() *(words.length))]
            }))
      }
      else if(this.state.wordLength == 4)
      {
          this.setState(state => ({
                randomWord: fourLetterWords[Math.floor(Math.random() *(fourLetterWords.length))]
            }))
      }
      else if(this.state.wordLength == 3)
      {
          this.setState(state => ({
                randomWord: threeLetterWords[Math.floor(Math.random() *(threeLetterWords.length))]
            }))
      }
    };
    gameStart = () =>  {
        this.rGWord();
        let i = this.state.guesses;
        if(this.state.wordLength == 5)
        {
            while(i > 1 )
            {
                guessArray5.splice(1, 0, 
                    {row: ['','','','',''], color: ['white','white','white','white','white']})
                i--;
            }
            this.setState(state => ({
                showGuessArray5: guessArray5,
            }))
            this.handleWordlePagePress()
        }
        else if(this.state.wordLength == 4)
        {
            while(i > 1 )
            {
                guessArray4.splice(1, 0, 
                    {row: ['','','',''], color: ['white','white','white','white']})
                i--;
            }
            this.setState(state => ({
                showGuessArray4: guessArray4,
            }))
            this.handleWordlePagePress4()
        }
        else if(this.state.wordLength == 3)
        {
            while(i > 1 )
            {
                guessArray3.splice(1, 0, 
                    {row: ['','',''], color: ['white','white','white']})
                i--;
            }
            this.setState(state => ({
                showGuessArray3: guessArray3,
            }))
            this.handleWordlePagePress3()
        }
    };
    addLetter = (letter) => {
        if(this.state.wordLength == 5)
        {
            if(this.state.currentCol < 5){
                guessArray5[this.state.currentRow].row[this.state.currentCol] =  letter;
                this.setState(state => ({
                    showGuessArray5: guessArray5,
                    currentCol: this.state.currentCol + 1,
                }))
            }
        }
        else if(this.state.wordLength == 4)
        {
            if(this.state.currentCol < 4){
                guessArray4[this.state.currentRow].row[this.state.currentCol] =  letter;
                this.setState(state => ({
                    showGuessArray4: guessArray4,
                    currentCol: this.state.currentCol + 1,
                }))
            }
        }
        else if(this.state.wordLength == 3)
        {
            if(this.state.currentCol < 3){
                guessArray3[this.state.currentRow].row[this.state.currentCol] =  letter;
                this.setState(state => ({
                    showGuessArray3: guessArray3,
                    currentCol: this.state.currentCol + 1,
                }))
            }
        }
    };
    enter = () => {
        if(this.state.wordLength == 5)
        {
            let word = guessArray5[this.state.currentRow].row.toString().replace(/,/g, '')
            for(let i = 0; i < words.length; i++)
                if(this.state.currentCol >= 5 && words[i].toUpperCase() == word)
                {
                    this.compareWord()
                    this.outcome(guessArray5)
                    this.setState(state => ({
                        currentRow: this.state.currentRow + 1,
                        currentCol: 0,
                    }))
                    break
                }
            
        }
        if(this.state.wordLength == 4)
        {
            let word = guessArray4[this.state.currentRow].row.toString().replace(/,/g, '')
            for(let i = 0; i < fourLetterWords.length; i++)
                if(this.state.currentCol >= 4 && fourLetterWords[i].toUpperCase() == word)
                {
                    this.outcome(guessArray4)
                    this.compareWord()
                    this.setState(state => ({
                        currentRow: this.state.currentRow + 1,
                        currentCol: 0,
                    }))
                }
        }
        if(this.state.wordLength == 3)
        {
            let word = guessArray3[this.state.currentRow].row.toString().replace(/,/g, '')
            for(let i = 0; i < threeLetterWords.length; i++)
                if(this.state.currentCol >= 3 && threeLetterWords[i].toUpperCase() == word)
                {
                    this.outcome(guessArray3)
                    this.compareWord()
                    this.setState(state => ({
                        currentRow: this.state.currentRow + 1,
                        currentCol: 0,
                    }))
                }
        }
    }
    back = () => {
        if(this.state.wordLength == 5)
        {
            if(this.state.currentCol >= 0){
                guessArray5[this.state.currentRow].row[this.state.currentCol - 1] = '';
                this.setState(state => ({
                    showGuessArray5: guessArray5,
                    currentCol: this.state.currentCol - 1,
                }))
            }
        }
        if(this.state.wordLength == 4)
        {
            if(this.state.currentCol >= 0){
                guessArray4[this.state.currentRow].row[this.state.currentCol - 1] = '';
                this.setState(state => ({
                    showGuessArray4: guessArray4,
                    currentCol: this.state.currentCol - 1,
                }))
            }
        }
        if(this.state.wordLength == 3)
        {
            if(this.state.currentCol >= 0){
                guessArray3[this.state.currentRow].row[this.state.currentCol - 1] = '';
                this.setState(state => ({
                    showGuessArray3: guessArray3,
                    currentCol: this.state.currentCol - 1,
                }))
            }
        }
    }
    compareWord = () => {
        if(this.state.wordLength == 5)
        {   
            let newStr = this.state.randomWord.toUpperCase();
            let arr = newStr.split("");
            for(let i = 0; i < 5; i++)
            {
                guessArray5[this.state.currentRow].color[i] = '#cccccc';
                
                if(arr[0] == guessArray5[this.state.currentRow].row[i] || arr[1] == guessArray5[this.state.currentRow].row[i] || arr[2] == guessArray5[this.state.currentRow].row[i] || arr[3] == guessArray5[this.state.currentRow].row[i] || arr[4] == guessArray5[this.state.currentRow].row[i])
                {
                    guessArray5[this.state.currentRow].color[i] = '#feffb8';
                    this.setState(state => ({
                    showGuessArray5: guessArray5,        
                    }))
                }
                if(arr[i] == guessArray5[this.state.currentRow].row[i])
                {
                    guessArray5[this.state.currentRow].color[i] = '#90d494';
                    this.setState(state => ({
                    showGuessArray5: guessArray5,        
                    }))
                }
            }
            
        }
        else if(this.state.wordLength == 4)
        {   
            let newStr = this.state.randomWord.toUpperCase();
            let arr = newStr.split("");
            for(let i = 0; i < 4; i++)
            {
                guessArray4[this.state.currentRow].color[i] = '#cccccc';
                
                if(arr[0] == guessArray4[this.state.currentRow].row[i] || arr[1] == guessArray4[this.state.currentRow].row[i] || arr[2] == guessArray4[this.state.currentRow].row[i] || arr[3] == guessArray4[this.state.currentRow].row[i])
                {
                    guessArray4[this.state.currentRow].color[i] = '#feffb8';
                    this.setState(state => ({
                    showGuessArray4: guessArray4,        
                    }))
                }
                if(arr[i] == guessArray4[this.state.currentRow].row[i])
                {
                    guessArray4[this.state.currentRow].color[i] = '#90d494';
                    this.setState(state => ({
                    showGuessArray5: guessArray4,        
                    }))
                }
            }
            
        }
        else if(this.state.wordLength == 3)
        {   
            let newStr = this.state.randomWord.toUpperCase();
            let arr = newStr.split("");
            for(let i = 0; i < 3; i++)
            {
                guessArray3[this.state.currentRow].color[i] = '#cccccc';
                
                if(arr[0] == guessArray3[this.state.currentRow].row[i] || arr[1] == guessArray3[this.state.currentRow].row[i] || arr[2] == guessArray3[this.state.currentRow].row[i])
                {
                    guessArray3[this.state.currentRow].color[i] = '#feffb8';
                    this.setState(state => ({
                    showGuessArray3: guessArray3,        
                    }))
                }
                if(arr[i] == guessArray3[this.state.currentRow].row[i])
                {
                    guessArray3[this.state.currentRow].color[i] = '#90d494';
                    this.setState(state => ({
                    showGuessArray3: guessArray3,        
                    }))
                }
            }
            
        }
    }
    changeWordLength = (value) => {
        if(value == 3){
            this.setState(state=> ({
                wordLength: 3,
                selectedL3: '#deebcc',
                selectedL4: 'white',
                selectedL5: 'white',
            }))
        }
        else if(value == 4){
            this.setState(state=> ({
                wordLength: 4,
                selectedL3: 'white',
                selectedL4: '#deebcc',
                selectedL5: 'white',
            }))
        }
        else if(value == 5){
            this.setState(state=> ({
                wordLength: 5,
                selectedL3: 'white',
                selectedL4: 'white',
                selectedL5: '#deebcc',
            }))
        }
    };
    
     numGameSelect = (value) => {
        if(value == 1){
            this.setState(state=> ({
                games: 1,
                selectedG1: '#deebcc',
                selectedG2: 'white',
                selectedG3: 'white',
                selectedG4: 'white',
            }))
        }
        else if(value == 2){
            this.setState(state=> ({
                games: 2,
                selectedG1: 'white',
                selectedG2: '#deebcc',
                selectedG3: 'white',
                selectedG4: 'white',
            }))
        }
        else if(value == 3){
            this.setState(state=> ({
                games: 3,
                selectedG1: 'white',
                selectedG2: 'white',
                selectedG3: '#deebcc',
                selectedG4: 'white',
            }))
        }
        else if(value == 4){
            this.setState(state=> ({
                games: 4,
                selectedG1: 'white',
                selectedG2: 'white',
                selectedG3: 'white',
                selectedG4: '#deebcc',
            }))
        }
    };
    

    render() {
        return (
            <View style={styles.container}>
