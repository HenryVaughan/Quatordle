                            <TouchableHighlight style={{height: deviceWidth/5, width: deviceWidth/5, borderWidth: 4, marginRight: 14, borderRadius: 90, alignItems: 'center', justifyContent: 'center', backgroundColor: this.state.selectedL5}}
